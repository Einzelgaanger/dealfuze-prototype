{
  "name": "@deal-fuze/server",
  "version": "1.0.0",
  "private": true,
  "main": "./dist/main.js",
  "types": "./dist/main.d.ts",
  "engines": {
    "node": "18.17.0",
    "npm": "9.6.7"
  },
  "scripts": {
    "dev": "nodemon -w . -x tsx src/main.ts | pino-pretty",
