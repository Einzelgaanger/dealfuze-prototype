{
  "name": "deal-fuze",
  "private": true,
  "workspaces": [
    "frontend",
    "server"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev --workspace=server\" \"npm run dev --workspace=frontend\"",
    "prebuild": "chmod +x ./build.sh && ./build.sh",
    "build": "npm run build --workspace=server && npm run build --workspace=frontend",
    "start": "npm run start --workspace=server",
    "typecheck": "concurrently \"npm run typecheck --workspace=server\" \"npm run typecheck --workspace=frontend\""
  },
  "dependencies": {
    "@nestjs/core": "^11.1.0",
    "@nestjs/platform-express": "^11.1.0",
    "@typegoose/typegoose": "^12.14.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "concurrently": "^8.2.2",
    "country-state-city": "^3.2.1",
    "mongodb": "^6.16.0",
    "mongoose": "^8.14.0"
  },
  "devDependencies": {
    "@clerk/express": "^1.4.9",
    "@formio/core": "^2.4.0",
    "@nestjs/common": "^11.1.0",
    "@nestjs/config": "^4.0.2",
    "@nestjs/mongoose": "^11.0.3",
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^5.0.1",
    "@types/express-validator": "^2.20.33",
    "@types/helmet": "^0.0.48",
    "@types/mongodb": "^4.0.6",
    "@types/mongoose": "^5.11.96",
    "@types/node": "^22.15.3",
    "@types/node-cron": "^3.0.11",
    "@types/pino": "^7.0.4",
    "@types/pino-http": "^5.8.4",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "node-cron": "^3.0.3",
    "openai": "^4.96.0",
    "path": "^0.12.7",
    "pino": "^9.6.0",
    "pino-http": "^10.4.0",
    "rxjs": "^7.8.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3",
    "zod": "^3.24.3"
  }
}
